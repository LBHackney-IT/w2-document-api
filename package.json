{
  "name": "w2-document-api",
  "version": "1.0.0",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.18.2",
    "serverless": "^1.60.5",
    "serverless-offline": "^5.12.1",
